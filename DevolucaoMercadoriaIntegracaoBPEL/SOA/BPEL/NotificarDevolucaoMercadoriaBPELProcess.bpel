<process name="NotificarDevolucaoMercadoriaBPELProcess"
               targetNamespace="http://xmlns.oracle.com/ExtrafarmaSOA_F1_S2/DevolucaoMercadoriaIntegracaoBPEL/DevolucaoMercadoriaBPELProcess"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/ExtrafarmaSOA_F1_S2/DevolucaoMercadoriaIntegracaoBPEL/DevolucaoMercadoriaBPELProcess"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
               xmlns:ns1="http://service.ultra.com.br/logistica/DevolucaoMercadoria/v1"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ui="http://xmlns.oracle.com/soa/designer"
         xmlns:ns2="http://www.extrafarma.com.br/sca/soapservice/ExtrafarmaSOA-F1-S1/RegistroLogLBBpel/RegistroLogWS"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ess="http://xmlns.oracle.com/scheduler" xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns3="http://service.ultra.com.br/infcorp/LogIntegration/v1"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/jms/AppExtrafarmaSOA/DevolucaoMercadoriaIntegracaoBPEL/EtRTVJMSReference"
         xmlns:ns5="http://www.oracle.com/retail/integration/base/bo/RTVDesc/v1"
         xmlns:ns7="http://www.oracle.com/retail/integration/base/bo/EXFItemDesc/v1"
         xmlns:ns6="http://www.oracle.com/retail/integration/custom/bo/ExtOfRTVDesc/v1"
         xmlns:ns8="http://www.oracle.com/retail/integration/rib/RibMessages"
         xmlns:ns9="http://canonico.ultra.com.br/infcorp/v1">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://service.ultra.com.br/logistica/DevolucaoMercadoria/v1"
          location="../WSDLs/DevolucaoMercadoriaV1.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/jms/AppExtrafarmaSOA/DevolucaoMercadoriaIntegracaoBPEL/EtRTVJMSReference"
          location="../WSDLs/EtRTVJMSReference.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"
          ui:processWSDL="true"/>
  <import namespace="http://service.ultra.com.br/infcorp/LogIntegration/v1"
          location="oramds:/apps/Extrafarma/Retail/Comum/v1/Schemas/MetaInformacao.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://www.extrafarma.com.br/sca/soapservice/ExtrafarmaSOA-F1-S1/RegistroLogLBBpel/RegistroLogWS"
          location="oramds:/apps/Extrafarma/Retail/Comum/Log/v1/WSDL/RegistroLogBPELWrapper.wsdl"
          importType="http://schemas.xmlsoap.org/wsdl/"/>
  <import namespace="http://www.oracle.com/retail/integration/rib/RibMessages"
          location="../Schemas/RibMessages.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://www.oracle.com/retail/integration/base/bo/RTVDesc/v1"
          location="oramds:/apps/Extrafarma/Retail/Comum/v1/Schemas/RTVDesc.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="EtRTVJMSReference" partnerLinkType="ns4:Posta_Mensagem_plt" partnerRole="Posta_Mensagem_role"/>
    <partnerLink name="RegistroLogBPELReference" partnerLinkType="ns2:RegistroLogBPELReference"
                 partnerRole="execute_ptt"/>
    <partnerLink name="DevolucaoMercadoriaDBBPEL" partnerLinkType="ns1:DevolucaoMercadoriaDBBPEL"
                 myRole="DevolucaoMercadoriaPortType"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="inputVariable" messageType="ns1:NotificarDevolucaoMercadoriaRequest"/>
    <variable name="outputVariable" messageType="ns1:NotificarDevolucaoMercadoriaResponse"/>
    <variable name="PostaMensagemEtRTVJMS_Posta_Mensagem_InputVariable" messageType="ns4:Posta_Mensagem_msg"/>
    <variable name="MetaInformacaovariable" element="ns3:MetaInformacao"/>
    <variable name="postaMensagemErroRegistroLogBPEL_execute_InputVariable" messageType="ns2:requestMessage"/>
    <variable name="RegistroLogBPEL_execute_InputVariable" messageType="ns2:requestMessage"/>
    <variable name="NotificarDevolucaoMercadoriaFault" messageType="ns1:NotificarDevolucaoMercadoriaFaultMsg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="FluxoTratamentoMensagem">
        <scope name="ScopeTratamentoMensagem">
          <faultHandlers>
            <catchAll>
              <sequence name="SequenceTratarFalha">
                <assign name="AssignFault">
                  <copy>
                    <from>'Ocorreu um erro no envio da mensagem ao Retail e no Registro de Log.'</from>
                    <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$NotificarDevolucaoMercadoriaFault.NotificarDevolucaoMercadoriaFault/ns9:mensagem</to>
                  </copy>
                  <copy>
                    <from>'ERRO_DE_SISTEMA'</from>
                    <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$NotificarDevolucaoMercadoriaFault.NotificarDevolucaoMercadoriaFault/ns9:tipo</to>
                  </copy>
                  <copy>
                    <from>ora:getFaultAsString()</from>
                    <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$NotificarDevolucaoMercadoriaFault.NotificarDevolucaoMercadoriaFault/ns9:detalhe</to>
                  </copy>
                  <copy>
                    <from>'500'</from>
                    <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$NotificarDevolucaoMercadoriaFault.NotificarDevolucaoMercadoriaFault/ns9:codigo</to>
                  </copy>
                </assign>
                <throw name="Throw" faultName="ns1:NotificarDevolucaoMercadoriaFault"
                       faultVariable="NotificarDevolucaoMercadoriaFault"/>
              </sequence>
            </catchAll>
          </faultHandlers>
          <sequence>
            <assign name="AssignPayloadToRegistroErroLogBPEL">
              <copy>
                <from>'DevolucaoMercadoriaBPELErro'</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:passoFluxo</to>
              </copy>
              <copy>
                <from>ora:getFaultAsString()</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:mensagem</to>
              </copy>
              <copy>
                <from>'RIB'</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:sistemaDestino</to>
              </copy>
              <copy>
                <from>oraext:get-content-as-string($inputVariable)</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Body</to>
              </copy>
              <copy>
                <from>'DevolucaoMercadoriaRestHTTP'</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:nomeServico</to>
              </copy>
              <copy>
                <from>$MetaInformacaovariable/ns3:datahoraRequisicao</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:datahoraRequisicao</to>
              </copy>
              <copy>
                <from>$MetaInformacaovariable/ns3:usuario</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:usuario</to>
              </copy>
              <copy>
                <from>$MetaInformacaovariable/ns3:idControle</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:idControle</to>
              </copy>
              <copy>
                <from>$MetaInformacaovariable/ns3:sistemaOrigem</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:sistemaOrigem</to>
              </copy>
              <copy>
                <from>1</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$postaMensagemErroRegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:status</to>
              </copy>
            </assign>
            <invoke name="postaMensagemErroRegistroLogBPEL" partnerLink="RegistroLogBPELReference"
                    portType="ns2:execute_ptt" operation="execute"
                    inputVariable="postaMensagemErroRegistroLogBPEL_execute_InputVariable" bpelx:invokeAsDetail="no"/>
          </sequence>
        </scope>
        <assign name="AssignReplyCatch">
          <copy>
            <from>'200 - OK'</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.parameters/out</to>
          </copy>
        </assign>
        <reply name="replyCatchOutput" partnerLink="DevolucaoMercadoriaDBBPEL"
               variable="outputVariable"
               portType="ns1:DevolucaoMercadoriaPortType" operation="notificarDevolucaoMercadoria"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in DevolucaoMercadoriaBPELProcess.wsdl) -->
    <receive name="receiveInput" partnerLink="DevolucaoMercadoriaDBBPEL" portType="ns1:DevolucaoMercadoriaPortType" operation="notificarDevolucaoMercadoria" variable="inputVariable" createInstance="yes"
             bpelx:headerVariable="MetaInformacaovariable"/>
    <sequence name="SequencePostaMensagemRib">
      <assign name="TransformationNotificaDevolucaoMercadoriaToRTVDesc">
        <bpelx:annotation>
          <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
        </bpelx:annotation>
        <copy>
          <from>ora:doXSLTransformForDoc("../Transformations/transformNotificaDevolucaoMercadoriaToPostaMensagem.xsl", $inputVariable.parameters)</from>
          <to variable="PostaMensagemEtRTVJMS_Posta_Mensagem_InputVariable" part="body"/>
        </copy>
      </assign>
      <invoke name="PostaMensagemEtRTVJMS" partnerLink="EtRTVJMSReference"
              portType="ns4:Posta_Mensagem_ptt" operation="Posta_Mensagem"
              inputVariable="PostaMensagemEtRTVJMS_Posta_Mensagem_InputVariable" bpelx:invokeAsDetail="no"/>
    </sequence>
    <sequence name="FluxoRegistraLog">
      <assign name="AssignPayloadToRegistroLogBPEL">
        <copy>
          <from>'RIB'</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:sistemaDestino</to>
        </copy>
        <copy>
          <from>0</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:status</to>
        </copy>
        <copy>
          <from>'DevolucaoMercadoriaRestHTTP'</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:nomeServico</to>
        </copy>
        <copy>
          <from>$MetaInformacaovariable/ns3:datahoraRequisicao</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:datahoraRequisicao</to>
        </copy>
        <copy>
          <from>$MetaInformacaovariable/ns3:usuario</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:usuario</to>
        </copy>
        <copy>
          <from>$MetaInformacaovariable/ns3:idControle</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:idControle</to>
        </copy>
        <copy>
          <from>oraext:get-content-as-string($inputVariable)</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Body</to>
        </copy>
        <copy>
          <from>'DevolucaoMercadoriaIntegracaoBPELProcessed'</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:passoFluxo</to>
        </copy>
        <copy>
          <from>'SUCESSO'</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:mensagem</to>
        </copy>
        <copy>
          <from>$MetaInformacaovariable/ns3:sistemaOrigem</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$RegistroLogBPEL_execute_InputVariable.part1/ns3:Header/ns3:MetaInformacao/ns3:sistemaOrigem</to>
        </copy>
      </assign>
      <invoke name="RegistroLogBPEL" partnerLink="RegistroLogBPELReference" portType="ns2:execute_ptt"
              operation="execute" inputVariable="RegistroLogBPEL_execute_InputVariable" bpelx:invokeAsDetail="no"/>
    </sequence>
    <assign name="AssignReplyOK">
      <copy>
        <from>'200 - OK'</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.parameters/out</to>
      </copy>
    </assign>
    <reply name="Reply" partnerLink="DevolucaoMercadoriaDBBPEL" variable="outputVariable"
           portType="ns1:DevolucaoMercadoriaPortType" operation="notificarDevolucaoMercadoria"/>
  </sequence>
</process>